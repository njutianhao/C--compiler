FUNCTION fact :
PARAM v1
v2 := #1
v3 := #1
LABEL l1 :
IF v3 <= v1 GOTO l2
GOTO l3
LABEL l2 :
t6 := v2 * v3
v2 := t6
t12 := v3 + #1
v3 := t12
GOTO l1
LABEL l3 :
RETURN v2
FUNCTION main :
DEC v4 20
v5 := #0
v6 := #5
LABEL l4 :
IF v5 < v6 GOTO l5
GOTO l6
LABEL l5 :
t26 := v5 * #4
t27 := &v4
t22 := t27 + t26
t23 := v5 + #2
*t22 := t23
t36 := v5 * #4
t37 := &v4
t33 := t37 + t36
t38 := *t33
ARG t38
t32 := CALL fact
WRITE t32
t40 := v5 + #1
v5 := t40
GOTO l4
LABEL l6 :
RETURN #0
